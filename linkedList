class Header:
    def __init__(self):
        self.length = 0
        self.head = None
        self.end = None
        self.prevNode = None
        self.posInList = None

    def addItem(self, value):
        k = Node(value)
        self.length += 1
        self.end = k

        if self.length == 1:
            self.head = k
            self.posInList = k
        else:
            self.prevNode.nextNode = k

        self.prevNode = k

    def printList(self):
        while True:
            print(self.posInList.value)
            if self.posInList == self.end:
                break
            self.posInList = self.posInList.nextNode



class Node:
    def __init__(self, value):
        self.value = value
        self.nextNode = None


list1 = Header()
list1.addItem(1)
list1.addItem(3)
list1.addItem(True)
list1.addItem(78)
list1.addItem(2)
list1.addItem("test")

#print(list1.length)

list1.printList()
    
